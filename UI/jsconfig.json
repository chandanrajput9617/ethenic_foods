{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      // "@/*": ["./src/*"],
    },
    "module": "esnext",
    "target": "ESNext",
    "moduleResolution": "Node",
  },
  "exclude": [
    "node_modules"
  ]
}